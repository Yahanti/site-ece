{
  "rewrites": [
    {
      "source": "/enviar",
      "destination": "/api"
    },
    {
      "source": "/dados",
      "destination": "/api"
    }
  ],
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/enviar",
      "dest": "/api"
    },
    {
      "src": "/dados",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}